{
  "schemaVersion": 1,
  "id": "thunderforge",
  "version": "${version}",
  "name": "Thunderforge",
  "description": "Mod Description",
  "authors": [
    "Author"
  ],
  "contact": {
    "homepage": "https://example.com/",
    "issues": "https://example.com/",
    "sources": "https://example.com/"
  },
  "license": "The Unlicense",
  "icon": "assets/thunderforge/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.curiouslad.thunderforge.Thunderforge"
      }
    ],
    "client": [{
      "adapter": "kotlin",
      "value": "com.curiouslad.thunderforge.client.ThunderforgeClient"
    }
    ]
  },
  "mixins": [
    "thunderforge.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabricloader}",
    "fabric-api": ">=${fabric_api}",
    "fabric-language-kotlin": ">=${fabric_language_kotlin}",
    "minecraft": ">=${minecraft}",
    "java": ">=${java}"
  },
  "accessWidener" : "thunderforge.accesswidener"

}